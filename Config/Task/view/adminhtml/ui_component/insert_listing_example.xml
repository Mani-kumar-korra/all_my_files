<?xml version="1.0" encoding="UTF-8"?>

<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">insert_listing_example.config_task_insert_listing_example_data_source</item>
        </item>
    </argument>
    <settings>
        <spinner>columns</spinner>
        <deps>
            <dep>insert_listing_example.config_task_insert_listing_example_data_source</dep>
        </deps>
    </settings>
    <dataSource name="config_task_insert_listing_example_data_source" component="Magento_Ui/js/grid/provider">
        <settings>
            <updateUrl path="mui/index/render"/>
        </settings>
        <dataProvider class="Config\Task\Ui\Component\Listing\CustomDataProvider" name="config_task_insert_listing_example_data_source">
            <settings>
                <requestFieldName>Key</requestFieldName>
                <primaryFieldName>Key</primaryFieldName>
            </settings>
        </dataProvider>
    </dataSource>
    <listingToolbar name="listing_top">
        <columnsControls name="columns_controls"/>
        <filterSearch name="fulltext"/>
        <filters name="listing_filters"/>
    </listingToolbar>
    <columns name="columns">
        
        <column name="parent_key" sortOrder="20">
        <settings>
            <filter>textRange</filter>
            <label translate="true">Parent Key</label>
            <sorting>asc</sorting>
        </settings>
    </column>
    <column name="Key" sortOrder="30">
        <settings>
            <filter>text</filter>
            <label translate="true">Key</label>
        </settings>
    </column>
    <column name="value" sortOrder="40">
        <settings>
            <filter>text</filter>
            <label translate="true">value</label>
        </settings>
    </column>
 
  
    </columns>
</listing>
